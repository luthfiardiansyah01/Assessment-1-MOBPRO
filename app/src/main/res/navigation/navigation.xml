<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/hitungFragment">

    <fragment
        android:id="@+id/hitungFragment"
        android:name="org.d3if1136.kursusutbk.ui.hitung.HitungFragment"
        tools:layout="@layout/fragment_hitung">
        <action
            android:id="@+id/action_hitungFragment_to_fasilitasFragment"
            app:destination="@id/fasilitasFragment" />
        <action
            android:id="@+id/action_hitungFragment_to_aboutFragment"
            app:destination="@id/aboutFragment" />
        <action
            android:id="@+id/action_hitungFragment_to_historiFragment"
            app:destination="@id/historiFragment" />
        <action
            android:id="@+id/action_hitungFragment_to_mainFragment"
            app:destination="@id/mainFragment" />
    </fragment>

    <fragment
        android:id="@+id/fasilitasFragment"
        android:name="org.d3if1136.kursusutbk.ui.FasilitasFragment"
        tools:layout="@layout/fragment_fasilitas" >
        <argument
            android:name="kategori"
            app:argType="org.d3if1136.kursusutbk.model.KategoriUtbk" />
    </fragment>
    <fragment
        android:id="@+id/aboutFragment"
        android:name="org.d3if1136.kursusutbk.ui.AboutFragment"
        tools:layout="@layout/fragment_about" />
    <fragment
        android:id="@+id/historiFragment"
        android:name="org.d3if1136.kursusutbk.ui.history.HistoriFragment"
        tools:layout="@layout/fragment_histori" />
    <fragment
        android:id="@+id/mainFragment"
        android:name="org.d3if1136.kursusutbk.ui.course.MainFragment"
        tools:layout="@layout/fragment_main" >
    </fragment>
</navigation>